<div class="text-center">
  <div class="d-flex justify-content-center">
<h1> Hey {{profLoop.firstName}}...Sup?</h1>
</div>
<div class="container">
Instruments:
<ol class="text-left">
  {{#each profLoop.instruments}}
  <li >{{this}}</li>
  {{/each}}
</ol>
</div>
</div>
<div class="user-bands">
  <h6>Bands:</h6>
  <ul>
    {{#each profLoop.bands}}
      <li><div><a href="/band-detail/{{this._id}}">{{this.bandName}}</a></div></li>
    {{/each}}
  </ul> 
  <h6>Admin of:</h6>
  <ul>
    {{#each profLoop.adminOf}}
      <li><div><a href="/band-profile/{{this._id}}">{{this.bandName}}</a></div></li>
    {{/each}}
  </ul>
  <div class="container">
  <a href="/create-band"><button type="submit" class="btn rounded-btns-red">Create A Band</button></a>
  <a href="/find-band"><button type="submit" class="btn rounded-btns-white">Join A Band</button></a>
  </div>
</div>

<hr class="line">
<div>
<a href="/edit-profile"><button type="submit" class="btn rounded-btns-red">Edit Your Rockfile!</button></a>

<a href="/logout"><button type="submit" class="btn rounded-btns-white">Rock Out!</button></a>
</div>

